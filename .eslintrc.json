{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "wrap-iife": ["error", "inside"],
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "indent": "off",
        "operator-linebreak": "warn",
        "implicit-arrow-linebreak": "warn",
        "no-param-reassign": "warn",
        "no-return-assign": "warn",
        "comma-dangle": "warn",
        "object-curly-newline": "warn"
    }
}
